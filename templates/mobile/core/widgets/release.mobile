{% extends "mobile/base_widget.mobile" %}

{% block widget_title %}Active Support Tickets{% endblock %}

{% block widget_content %}
<div  class="larger spaced-h spaced-v" style="color: #072">
    <strong>
        {{ release['days'] }} days,
        {{ release['hours'] }} hours,
        {{ release['minutes'] }} minutes,
        {{ release['seconds'] % 60 }} seconds
